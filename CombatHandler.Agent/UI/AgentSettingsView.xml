<?xml version="1.0" standalone="yes"?>
<root>
<View h_alignment="LEFT" view_layout="vertical" name="parent">
	<BorderView v_alignment="TOP" h_alignment="LEFT" view_layout="vertical" name="Settings" layout_borders="Rect(5,5,5,5)" max_size="Point(16000,-1)">
		<TextView value="Settings" layout_borders="Rect(5,5,5,5)"/>
		<OptionCheckBox label="Use DoT Strain A on fighting target" opt_category="AOSharp" opt_type="variant" opt_variable="CombatHandler__UseDotStrainA" layout_borders="Rect(10,1,5,1)"/>
		<OptionCheckBox label="Debuff target evasion" opt_category="AOSharp" opt_type="variant" opt_variable="CombatHandler__UseEvasionDebuff" layout_borders="Rect(10,1,5,1)"/>
		<BitmapView bitmap_id="GFX_GUI_FRIENDLIST_SPLITTER" layout_borders="Rect(5,5,5,5)"/>
		<OptionRadioButtonGroup label="Proc selection" layout_borders="Rect(10,1,5,1)" opt_type="variant" opt_variable="CombatHandler__ProcSelection">
			<RadioButton label="Use Detaunt proc" value="0"/>
			<RadioButton label="Use DoT proc" value="1"/>
		</OptionRadioButtonGroup>
		<OptionRadioButtonGroup label="LE Proc selection" layout_borders="Rect(10,1,5,1)" opt_type="variant" opt_variable="CombatHandler__LEProcSelection">
			<RadioButton label="Use Laser Aim" value="0"/>
			<RadioButton label="Use Notum Charged Rounds" value="1"/>
		</OptionRadioButtonGroup>
		<VLayoutSpacer/>
	</BorderView>
</View>
</root>